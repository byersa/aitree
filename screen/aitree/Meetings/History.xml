<?xml version="1.0" encoding="UTF-8"?>
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="component://moqui-ai/xsd/moqui-ai-screen.xsd"
        require-authentication="false">
    <widgets>
        <container class="q-py-md">
            <container-row>
                <row-col md="12">
                    <label text="Meeting History" class="text-h6 q-mb-md"/>
                    <label text="Review past clinical huddles and coordination meetings." class="text-body2 text-grey-8"/>
                </row-col>
            </container-row>
            <q-separator class="q-my-md"/>
            <q-banner class="bg-grey-2 text-grey-8 rounded-borders">
                <template v-slot:avatar=""><q-icon name="history" color="grey-8" /></template>
                <label text="History populated from past clinical events."/>
            </q-banner>
        </container>
    </widgets>
</screen>
